<!DOCTYPE html>
<html>
  <head>
    <title>FacebookLike</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
      <%= render 'layouts/nav' %>
    <% end %>

    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
