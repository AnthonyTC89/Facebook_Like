<!DOCTYPE html>
<html>
  <head>
    <title>FacebookLike</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
      <div>ID: <%= current_user.id %></div>
      <%= link_to 'Profile', '/users/show' %>
      <%= link_to 'Edit User', '/users/edit' %>
      <%= link_to 'Logout', '/users/sign_out', method: :delete %>
    <% end %>
    <%= yield %>
  </body>
</html>
