<h2> Please create your account</h2><br/> <br/>
  <%= form_for :@user do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %><br/><br/> 
    <%= f.label :last_name %>
    <%= f.text_field :last_name %><br/><br/> 
    <%= f.label :gender %>
    <%= f.text_field :gender %><br/><br/> 
    <%= f.label :birthday %>
    <%= f.date_field :birthday %><br/><br/> 
    <%= f.label :email %>
    <%= f.text_field :email %><br/><br/> 
    <%= f.label :password %>
    <%= f.text_field :password %><br/><br/> 
    <%= f.submit value="Signup"%>
  <% end %><br/> <br/>
  <h2><a href='/auth/facebook'>Sign Up with Facebook</a></h2>
